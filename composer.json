{
    "name": "mohdnazrul/laravel-ctos-kyc",
    "description": "Library to pull reports from CTOS KYC System",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "illuminate/support": "5.4.*",
        "phpunit/phpunit": "^7.0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mohd Nazrul Bin Mustaffa",
            "email": "nazrulmustaffa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MohdNazrul\\CTOSKYCLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohdNazrul\\CTOSKYCLaravel\\CTOSKYCServiceProvider"
            ],
            "aliases": {
                "CTOSKYC": "MohdNazrul\\CTOSKYCLaravel\\CTOSKYCApiFacade"
            }
        }
    }
}
